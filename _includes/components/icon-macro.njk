{% macro icon(data, name) %}{% if data %}<div class="position-relative icon-container">{% if data.imageurl %}<div class="icon icon-framed{% if data.imagecrop %} icon-cropped{% endif %}"><img {% if name %}alt="Icon for {{ name }}" {% endif %}src="{{ data.imageurl }}"></div>{% elseif data.faSolid %}<div class="icon icon-monochrome">{% faSolid data.faSolid %}</div>{% elseif data.faBrand %}<div class="icon icon-monochrome">{% faBrand data.faBrand %}</div>{% endif %}</div>{% endif %}{% endmacro %}