{% macro icon(data, name) %}{% if data.imageurl %}<span class="icon icon-framed{% if data.imagecrop %} icon-cropped{% endif %}"><img {% if name %}alt="{{ name }}" {% endif %}src="{{ data.imageurl }}"></span>{% elseif data.icon %}<span class="icon icon-monochrome">{% icon data.icon %}</span>{% elseif data.faSolid %}<span class="icon icon-monochrome">{% faSolid data.faSolid %}</span>{% elseif data.faBrand %}<span class="icon icon-monochrome">{% faBrand data.faBrand %}</span>{% endif %}{% endmacro %}