<div class="theme-mode-tooltip-container"></div>
<button class="theme-mode-toggle-container" onclick="toggleThemeMode()" data-bs-toggle="tooltip" data-bs-placement="left" data-bs-trigger="hover" data-bs-container=".theme-mode-tooltip-container" title="Theme">
	<div class="theme-mode-toggle">
		<div class="theme-mode-toggle-circle-inner">
			<div class="theme-mode-toggle-inner-left">
				{% for num in range(1, 8) %}<div class="theme-mode-toggle-line theme-mode-toggle-line-{{ num }}"></div>{% endfor %}
			</div><div class="theme-mode-toggle-inner-right">
				{% for num in range(1, 8) %}<div class="theme-mode-toggle-line theme-mode-toggle-line-{{ num }}"></div>{% endfor %}
			</div>
		</div>
	</div>
	<span aria-hidden="false" class="align-text-top d-md-none">Theme</span>
</button>
